{
  "name": "hls",
  "displayName": "HLS",
  "description": "HTTP Live Streaming syntax highlighting",
  "version": "0.1.0",
  "publisher": "Lunkers",
  "author": {
    "name": "Fredrik Lundkvist"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "license": "MIT",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "HLS","hls","HTTP live streaming", "manifest", "Streaming"
  ],
  "main": ".out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "hls",
        "aliases": [
          "HLS",
          "hls"
        ],
        "extensions": [
          ".m3u8"
        ]
      }
    ],
    "grammars": [
      {
        "language": "hls",
        "scopeName": "source.hls",
        "path": "./syntaxes/hls.tmLanguage.json"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/Lunkers/hls-vscode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lunkers/hls-vscode.git"
  },
  "homepage": "https://github.com/Lunkers/hls-vscode/blob/main/README.md"
}